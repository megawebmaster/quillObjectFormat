{
  "name": "taggable-object-quill-format",
  "version": "0.1.0",
  "authors": [
    "Amadeusz Starzykiewicz <megawebmaster@gmail.com>"
  ],
  "description": "Configurable object format for Quill. Adds ability to format text as \"object\" where you define what an object is.",
  "main": "src/module.coffee",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/megawebmaster/taggableObjectQuillFormat.git"
  },
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "quill",
    "format",
    "agh",
    "objects",
    "configurable"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests",
    "coffeelint.json",
    "Gulpfile.coffee",
    "Gulpfile.js",
    "package.json"
  ],
  "dependencies": {
    "quill": "megawebmaster/quill#dist",
    "ngQuill": "megawebmaster/ngQuill#master",
    "jquery": "2.1.4"
  },
  "override": {
    "quill": {
      "main": ["dist/quill.js", "dist/quill.base.css"]
    },
    "ng-quill": {
      "main": "src/ng-quill.min.js"
    }
  },
  "resolve": {
    "quill": "dist"
  }
}
